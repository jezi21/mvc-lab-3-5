<%- include('partials/navigations') %>
<h1>Books</h1>
<% if (books.length === 0) { %>
    <p>No books has been found</p>
<% } else { %>
    <ul>
        <% books.forEach(book => { %>
            <li><%= book.title %> by <%= book.author %>, published in <%= book.publishingYear %></li>
        <% }) %>
    </ul>
<% } %>